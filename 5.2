
largest = none
smallest = none

while True:
	line = input('Enter a number: ')
	try:
		number = float(line) 
		if number = none or number > largest
			largest = number
		if smallest = none or number < smallest
			smallest = number
	except:
		if line == 'done':
			break		 
		else: 
			print('Invalid') 

print(largest, smallest)



